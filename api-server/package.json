{
  "name": "walgraph-enterprise-api",
  "version": "1.0.0",
  "description": "Enterprise-grade decentralized graph database API built on SUI blockchain and Walrus protocol",
  "main": "dist/app.js",
  "scripts": {
    "start": "node ./dist/bin/www.js",
    "dev": "ts-node-dev --respawn --transpile-only ./bin/www.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "docker:build": "docker build -t walgraph-api .",
    "docker:run": "docker run -p 3000:3000 walgraph-api",
    "deploy:prod": "npm run build && npm start",
    "clean": "rm -rf dist/",
    "prebuild": "npm run clean"
  },
  "dependencies": {
    "@mysten/sui": "^1.24.0",
    "aws-sdk": "^2.1490.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "cron": "^3.1.6",
    "debug": "~2.6.9",
    "dotenv": "^16.3.1",
    "express": "~4.16.1",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonld": "^8.3.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "morgan": "~1.9.1",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.9.7",
    "prom-client": "^15.0.0",
    "rate-limiter-flexible": "^4.0.1",
    "redis": "^4.6.10",
    "stripe": "^14.7.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.11.0",
    "zod": "^4.0.8"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/cron": "^2.4.0",
    "@types/debug": "^4.1.12",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.8",
    "@types/jsonld": "^1.5.15",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^1.4.11",
    "@types/node": "^24.1.0",
    "@types/nodemailer": "^6.4.14",
    "@types/supertest": "^2.0.16",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "graph-database",
    "blockchain",
    "sui",
    "walrus",
    "decentralized",
    "api",
    "enterprise"
  ],
  "author": "WalGraph Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/walgraph/enterprise-api"
  }
}
